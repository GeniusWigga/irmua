{
  "name": "gatsby-starter-default",
  "description": "Gatsby default starter",
  "version": "1.0.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "gatsby": "^1.9.73",
    "gatsby-link": "^1.6.22",
    "gatsby-plugin-react-helmet": "^1.0.8"
  },
  "keywords": [
    "gatsby"
  ],
  "engines": {
    "node": "8.6.0"
  },
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "npm run check && npm run clean && gatsby build",
