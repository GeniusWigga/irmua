{
  "name": "gatsby-starter-default",
  "description": "Gatsby default starter",
  "version": "1.0.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "gatsby": "^1.9.73",
    "gatsby-link": "^1.6.22",
    "gatsby-plugin-react-helmet": "^1.0.8",
    "imagesloaded": "^4.1.3",
    "lodash": "^4.17.4",
    "masonry-layout": "^4.2.0",
    "swiper": "^4.0.2"
  },
  "keywords": [
    "gatsby"
  ],
  "engines": {
    "node": "8.9.3"
  },
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "npm run check && npm run clean && npm run build:cms && npm run build:site",
    "build:site": "gatsby build",
    "build:cms": "cd cms && npm i && npm run build",
    "serve": "gatsby serve",
    "clean": "rm -rf public",
    "check": "node checkVersion.js",
    "develop": "npm run check && npm run clean && gatsby develop",
    "format": "npm run check && prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "prettier": "^1.7.4"
  }
}
